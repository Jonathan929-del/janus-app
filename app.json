{
  "expo": {
    "name": "Janus",
    "slug": "janus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/janus.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/janus.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Janus to access your photos.",
          "savePhotosPermission": "Allow Janus to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Janus to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Janus to save photos."
      },
      "bundleIdentifier": "janus.com"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/janus.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAAVvrfyRaRFBLMqDCIzw2O-qipJWHExHM"
        }
      },
      "package": "janus.com",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/janus.png"
    },
    "extra": {
      "eas": {
        "projectId": "46279cde-5588-454d-bda4-074e0fb78fc9"
      }
    }
  }
}
